<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dialog_select_hum"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    >
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/dialog_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/dialog_select_humidifier"
        android:paddingTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@color/transparent">
    </androidx.appcompat.widget.AppCompatImageView>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/hum_lv_1"
        android:layout_width="100dp"
        android:layout_height="120dp"
        app:layout_constraintTop_toTopOf="@id/dialog_background"
        app:layout_constraintStart_toStartOf="@id/dialog_background"
        android:layout_marginTop="85dp"
        android:layout_marginStart="25dp"
        android:background="@color/transparent"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/hum_t_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/hum_lv_1"
        app:layout_constraintEnd_toEndOf="@id/hum_lv_1"
        app:layout_constraintBottom_toBottomOf="@id/hum_lv_1"
        android:text="0Np"
        android:textColor="#353131"
        android:textStyle="bold"
        android:fontFamily="@font/onemobilefont"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/hum_lv_2"
        android:layout_width="100dp"
        android:layout_height="120dp"
        app:layout_constraintTop_toTopOf="@id/dialog_background"
        app:layout_constraintEnd_toEndOf="@id/dialog_background"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="25dp"
        android:background="@color/transparent"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/hum_t_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/hum_lv_2"
        app:layout_constraintEnd_toEndOf="@id/hum_lv_2"
        app:layout_constraintBottom_toBottomOf="@id/hum_lv_2"
        android:text="500Np"
        android:textColor="#353131"
        android:textStyle="bold"
        android:fontFamily="@font/onemobilefont"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/hum_lv_3"
        android:layout_width="100dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="@id/dialog_background"
        app:layout_constraintStart_toStartOf="@id/dialog_background"
        android:layout_marginBottom="65dp"
        android:layout_marginStart="25dp"
        android:background="#99000000"
        app:srcCompat="@drawable/lock"
        android:scaleType="center"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/hum_t_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/hum_lv_3"
        app:layout_constraintEnd_toEndOf="@id/hum_lv_3"
        app:layout_constraintBottom_toBottomOf="@id/hum_lv_3"
        android:text="1000Np"
        android:textColor="#353131"
        android:textStyle="bold"
        android:fontFamily="@font/onemobilefont"
        android:layout_marginBottom="3dp"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/hum_lv_4"
        android:layout_width="100dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="@id/dialog_background"
        app:layout_constraintEnd_toEndOf="@id/dialog_background"
        android:layout_marginBottom="65dp"
        android:layout_marginEnd="25dp"
        android:background="#99000000"
        app:srcCompat="@drawable/lock"
        android:scaleType="center"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/hum_t_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/hum_lv_4"
        app:layout_constraintEnd_toEndOf="@id/hum_lv_4"
        app:layout_constraintBottom_toBottomOf="@id/hum_lv_4"
        android:text="3000Np"
        android:textColor="#353131"
        android:textStyle="bold"
        android:fontFamily="@font/onemobilefont"
        android:layout_marginBottom="3dp"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/using1"
        android:layout_width="100dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="@id/hum_lv_1"
        app:layout_constraintStart_toStartOf="@id/hum_lv_1"
        app:layout_constraintTop_toTopOf="@id/hum_lv_1"
        app:layout_constraintEnd_toEndOf="@id/hum_lv_1"
        app:srcCompat="@drawable/using"
        android:visibility="invisible"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/using2"
        android:layout_width="100dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="@id/hum_lv_2"
        app:layout_constraintStart_toStartOf="@id/hum_lv_2"
        app:layout_constraintTop_toTopOf="@id/hum_lv_2"
        app:layout_constraintEnd_toEndOf="@id/hum_lv_2"
        app:srcCompat="@drawable/using"
        android:visibility="invisible"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/using3"
        android:layout_width="100dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="@id/hum_lv_3"
        app:layout_constraintStart_toStartOf="@id/hum_lv_3"
        app:layout_constraintTop_toTopOf="@id/hum_lv_3"
        app:layout_constraintEnd_toEndOf="@id/hum_lv_3"
        app:srcCompat="@drawable/using"
        android:visibility="invisible"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/using4"
        android:layout_width="100dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="@id/hum_lv_4"
        app:layout_constraintStart_toStartOf="@id/hum_lv_4"
        app:layout_constraintTop_toTopOf="@id/hum_lv_4"
        app:layout_constraintEnd_toEndOf="@id/hum_lv_4"
        app:srcCompat="@drawable/using"
        android:visibility="invisible"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/cancle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/alert_btn_clicked"
        app:layout_constraintStart_toStartOf="@id/dialog_background"
        app:layout_constraintEnd_toEndOf="@id/dialog_background"
        app:layout_constraintBottom_toBottomOf="@id/dialog_background"
        android:clickable="true"
        android:layout_marginBottom="17dp">
    </androidx.appcompat.widget.AppCompatImageButton>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/cancle"
        app:layout_constraintTop_toTopOf="@id/cancle"
        app:layout_constraintStart_toStartOf="@id/cancle"
        app:layout_constraintBottom_toBottomOf="@id/cancle"
        android:text="돌아가기"
        android:gravity="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16sp">
    </androidx.appcompat.widget.AppCompatTextView>


</androidx.constraintlayout.widget.ConstraintLayout>